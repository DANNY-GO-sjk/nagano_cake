<div id="search-bar">
  <%= form_with url: admins_search_path, method: :get, local: true do |f| %>
    <%= f.select :category, [["商品", "items"], ["顧客", "users"]] %>
    <%= f.text_field :str %>
    <button type="submit">
      <span class="glyphicon glyphicon-search"></span>
    </button>
  <% end %>
</div>
