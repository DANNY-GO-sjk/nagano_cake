<div class="container">
	<div class="row">
		<h3>会員一覧</h3>
	</div>
	<div class="row">
		<table class="table">
			<thead>
				<tr>
					<th>会員ID</th>
					<th>氏名</th>
					<th>メールアドレス</th>
					<th>ステータス</th>
				</tr>
			</thead>
			<tbody>
				<% @users.each do |user| %>
					<tr>
						<td>
							<%= user.id %>
						</td>
						<td>
							<%= "#{user.family_name} #{user.first_name}氏名未実装" %>
						</td>
						<td>
							<%= user.email %>
						</td>
						<td>
							<%= "#{user.is_valid}ステータス未実装" %>
						</td>
					</tr>
				<% end %>
			</tbody>
		</table>
	</div>
</div>
